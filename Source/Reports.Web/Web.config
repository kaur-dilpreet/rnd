  <log4net>
    <appender name="RollingFileTA" type="log4net.Appender.RollingFileAppender">
      <file value=".\Logs\ta-log.xml" />
      <appendToFile value="true" />
      <maximumFileSize value="2MB" />
      <maxSizeRollBackups value="2" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="&lt;%-5level&gt; %n    &lt;date&gt;%d&lt;/date&gt; %n    &lt;thread&gt;%t&lt;/thread&gt; %n    &lt;logger&gt;%c&lt;/logger&gt;%m%n&lt;/%-5level&gt;%n" />
      </layout>
      <filter type="log4net.Filter.LevelRangeFilter">
        <param name="LevelMin" value="DEBUG" />
        <param name="LevelMax" value="FATAL" />
      </filter>
    </appender>
    <appender name="RollingFileNH" type="log4net.Appender.RollingFileAppender">
      <file value=".\Logs\ta-NH-log.xml" />
      <appendToFile value="true" />
      <maximumFileSize value="2MB" />
      <maxSizeRollBackups value="2" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="&lt;%-5level&gt; %n    &lt;date&gt;%d&lt;/date&gt; %n    &lt;thread&gt;%t&lt;/thread&gt; %n    &lt;logger&gt;%c&lt;/logger&gt;%m%n&lt;/%-5level&gt;%n" />
      </layout>
      <filter type="log4net.Filter.LevelRangeFilter">
        <param name="LevelMin" value="DEBUG" />
        <param name="LevelMax" value="FATAL" />
      </filter>
    </appender>
    <logger name="TA">
      <level value="All" />
      <appender-ref ref="RollingFileTA" />
    </logger>
    <logger name="NHibernate">
      <level value="Error" />
      <appender-ref ref="RollingFileNH" />
    </logger>
    <logger name="NHibernate.SQL">
      <level value="Error" />
      <appender-ref ref="RollingFileNH" />
    </logger>
  </log4net>
